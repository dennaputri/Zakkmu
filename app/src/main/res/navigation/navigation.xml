<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/hitungFragment">
    <fragment
        android:id="@+id/hitungFragment"
        android:name="org.d3if0019.zakkmu.ui.HitungFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_hitung">
        <action
            android:id="@+id/action_hitungFragment_to_mahzabFragment"
            app:destination="@id/mahzabFragment" />
        <action
            android:id="@+id/action_hitungFragment_to_aboutFragment"
            app:destination="@id/aboutFragment" />

    </fragment>
    <fragment
        android:id="@+id/mahzabFragment"
        android:name="org.d3if0019.zakkmu.ui.MahzabFragment"
        tools:layout="@layout/fragment_mahzab"/>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="org.d3if0019.zakkmu.ui.AboutFragment"
        android:label="@string/tentang_aplikasi"
        tools:layout="@layout/fragment_about"/>
</navigation>